{
  "name": "gloden sentence",
  "version": "0.1",
  "description": "gloden sentence for V2EX",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_page": "options.html",
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/batman.png",
      "32": "images/batman.png",
      "48": "images/batman.png",
      "128": "images/batman.png"
    }
  },
  "icons": {
    "16": "images/batman.png",
    "32": "images/batman.png",
    "48": "images/batman.png",
    "128": "images/batman.png"
  },
  "manifest_version": 2
}
